<div class="booyah-box col-10 offset-1">
  <h1>Enter your place</h1>
    <%= simple_form_for @place do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <br>
        <%= f.submit 'Cancel', class: 'btn btn-primary' %>
        <%= f.submit 'Add this place', class: 'btn btn-primary' %>
    <% end %>
    </div>

